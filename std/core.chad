fn rev(range r) revRange
  ret { start = r.end - 1, end = r.start - 1, output = r.start - 1 }

struct revRange
  int start
  int end
  int output

fn next(&revRange r) *int
  int thisIndex = r.start
  if r.start == r.end
    ret nil
  r.start -= 1
  ret thisIndex
