struct range
  int start
  int end
  int output

impl next(&range r) *int
  r.output = r.start
  if r.start == r.end
    ret nil
  r.start += 1
  ret &r.output

struct bump
  *u8 base
  *u8 curr

local bump bp = {}

fn clear(&bump bp)
  int diff = int(u64(bp.curr) - u64(bp.base))
  memset(bp.base, u8(0), diff)
  bp.curr = bp.base

fn memset(*u8 mem, u8 val, int num)
  include
    memset(_mem, _val, _num);

fn alloc(&bump bp, int amt) *T
  int allocSize = 0
  include
    _allocSize = _amt * sizeof($(T));

  if bp.base == nil
    bp.base = malloc(1024 * 1024 * 1024)
    bp.curr = bp.base

  *T newLoc = ptr(bp.curr)
  newLoc = align(newLoc)
  bp.curr = ptr(&newLoc[amt])
  ret newLoc

fn alloc(int amt) *T
  ret alloc(bp, amt)

fn align(*T p) *T
  include
    size_t loc = (size_t)_p & ~alignof($(T));
    if (loc == (size_t)_p) return _p;
    return ($(T)*)(loc + sizeof($(T)));
  ret nil

fn malloc(int amt) *T
  include
    return malloc(_amt * sizeof($(T)));
  ret nil

fn free(*T mem)
  include
    free(_mem);
  ret nil

fn min(int a, int b) int
  if b < a
    ret b
  ret a

fn max(int a, int b) int
  if b > a
    ret b
  ret a

